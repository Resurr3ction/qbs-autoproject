headerdirs                     += .
sourcedirs                     += .
headers.fileextensions         += *.h
sources.fileextensions         += *.qdoc *.cpp
generateindex                   = true
syntaxhighlighting              = true
version                         = 1.0

Cpp.ignoretokens               += LIBRARY_SHARED \
                                  OTHERLIBRARY_SHARED

HTML.stylesheets                = $QTDIR/doc/global/template/style/offline.css
HTML.headerstyles               = "<link rel=\"stylesheet\" type=\"text/css\" href=\"style/offline.css\" />\n"

qhp.projects                    = FlatProject
qhp.FlatProject.file            = FlatProject.qhp
qhp.FlatProject.namespace       = org.examples.flatproject.1.0
qhp.FlatProject.virtualFolder   = flatproject
qhp.FlatProject.indexTitle      = FlatProject Reference Documentation
qhp.FlatProject.indexRoot       = FlatProject
qhp.extraFiles                 += style/offline.css
